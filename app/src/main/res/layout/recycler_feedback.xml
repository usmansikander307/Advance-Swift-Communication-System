        android:layout_below="@+id/etarid"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        android:textSize="25dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="70dp"
        android:id="@+id/txt44" />

    <TextView
